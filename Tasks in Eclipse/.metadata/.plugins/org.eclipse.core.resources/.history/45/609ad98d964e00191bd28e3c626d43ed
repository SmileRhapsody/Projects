package test;

import java.util.*;

import javax.lang.model.element.Element;

public class test1 {
	public void show(String s) {
		Map<String,Integer> map = new HashMap<>();
        map.put("I",1);
        map.put("V",5);
        map.put("X",10);
        map.put("L",50);
        map.put("C",100);
        map.put("D",500);
        map.put("M", 1000);

        for(int i=0;i<s.length();i++) {
        	int num = map.get(String.valueOf(s.charAt(i)));
        	System.out.println(num);
        }
	}
	
}
