package test;

import java.util.*;

import javax.lang.model.element.Element;

public class test1 {
	public void show(int x) {
		String s = Integer.toString(x);
		List<Character> list = new ArrayList<>();
		for(int i = s.length()-1; i>=0; i--) {
			list.add(s.charAt(i));
		}
		System.out.println(list);
		String s1="";
		for(char i:list) {
			s1 += i;
		}
		System.out.println(s1);
	}
	
}
