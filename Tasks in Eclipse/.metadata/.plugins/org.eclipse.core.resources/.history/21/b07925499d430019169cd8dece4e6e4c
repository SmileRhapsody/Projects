package test;

import java.util.*;

public class test1 {
	public void show(String s) {
		char[] chars = s.toCharArray();
		char[] newchar = new char[chars.length];
		List<Integer> dis = new ArrayList<>();
		for(int i = 0;i<chars.length;i++) {
			for(int j=i+1;j<chars.length;j++) {
				if(chars[i]==chars[j]) {
					dis.add(j-i);
					break;
				}
			}	
		}
		int max=0;
		for(int i:dis) {
			if(i>max) max=i;
		}
		System.out.println(max);
	}
	
}
