package test;

import java.util.*;

public class test1 {
	public void show(String s) {
		char[] chars = s.toCharArray();
		for(int i = 0;i<chars.length;i++) {
			for(int j=i+1;j<chars.length;j++) {
				if(chars[i]==chars[j]) {
					repeat = true;
                    break;
				}
			}
			if(repeat=true) break;
		}
		System.out.println(chars.length);
	}
	
}
